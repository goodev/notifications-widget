    <string name="tutorial_1_title_1">Welcome</string>
    <string name="tutorial_1_text_1">NiLS brings you the best way to get and interact with notifications on your lock screen</string>
    <string name="tutorial_1_lets_get_started">Let\'s Get Started!</string>
    <string name="tutorial_2_title_1">Start NiLS Service</string>
    <string name="tutorial_2_text_1">NiLS needs an access to your notifications</string>
    <string name="tutorial_2_text_2">Go to Android settings and enable NiLS service to grant this access</string>
    <string name="tutorial_2_text_3"><![CDATA[Hit the "Back" button on your device after enabling the service]]></string>
    <string name="tutorial_2_open_android_settings">Open Android Settings</string>
    <string name="tutorial_3_title_1">NiLS is Active</string>
    <string name="tutorial_3_text_1">NiLS is now active. all upcoming notifications will appear on your lock screen</string>
    <string name="tutorial_3_text_2">Swipe a notification to the left or to the right to dismiss it</string>
    <string name="tutorial_3_text_3">Swipe the notifications list down to dismiss all of the notifications</string>
    <string name="tutorial_4_title_1">Opening a Notification</string>
    <string name="tutorial_4_text_1">Touch and hold the notification icon to see the available actions</string>
    <string name="tutorial_4_text_2">Drag the dots circle to the rightmost action icon to open the notification</string>
    <string name="tutorial_5_title_1">Moving and Resizing</string>
    <string name="tutorial_5_text_1">Touch and hold the notifications list to move it, release to start resizing it</string>
    <string name="tutorial_6_title_1">That\'s All</string>
    <string name="tutorial_6_text_1"><![CDATA[Tap "Finish" to complete the tutorial, then tap on NiLS icon to get access to many options and customizations\n\nEnjoy the app!]]></string>
    <string name="tutorial_6_finish">Finish</string>
    <string name="enable_notifications_panel_text_1">NiLS v1.5 introduces a new and exciting way to get and interact with your notifications.</string>
    <string name="enable_notifications_panel_text_2">Enable Notifications Panel to see notifications on top of your lock screen without using a widget and enjoy swiping gestures, rich appearance customizations, themes support and many more options.</string>
